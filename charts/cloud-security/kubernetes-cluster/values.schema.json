{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "connector": {
      "identity": {
        "type": "boolean"
      },
      "network": {
        "type": "boolean"
      },
      "containerImagePath": {
        "type": "string"
      },
      "required": [
        "identity",
        "network",
        "containerImagePath"
      ],
      "type": "object"
    },
    "manager": {
      "containerImagePath": {
        "type": "string"
      },
      "required": [
        "containerImagePath"
      ],
      "type": "object"
    },
    "accountId": {
      "type": ["integer", "string"]
    },
    "apiKeyToken": {
      "type": "string"
    },
    "apiKeyTokenSecretName": {
      "type": "string"
    },
    "apiUrl": {
      "type": "string"
    },
    "proxyUrl": {
      "type": "string"
    }
  },
  "required": [
    "apiUrl",
    "containerImagePath"
  ],
  "type": "object"
}