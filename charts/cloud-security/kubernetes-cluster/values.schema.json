{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "properties": {
    "accountId": {
      "type": [
        "integer",
        "null",
        "string"
      ]
    },
    "admissionController": {
      "containerImagePath": {
        "type": "string"
      },
      "enabled": {
        "type": "boolean"
      },
      "healthPort": {
        "type": "integer"
      },
      "replicas": {
        "type": "integer"
      },
      "webhook": {
        "failurePolicy": {
          "type": "string"
        },
        "port": {
          "type": "integer"
        },
        "timeoutSeconds": {
          "type": "integer"
        },
        "type": "object"
      },
      "tls": {
        "serverCertificateBase64": {
          "type": "string"
        },
        "serverCertificateAuthorityCertificateBase64": {
          "type": "string"
        },
        "serverCertificateKeyBase64": {
          "type": "string"
        },
        "type": "object"
      },
      "type": "object"
    },
    "apiKeyToken": {
      "type": "string"
    },
    "apiKeyTokenSecretName": {
      "type": "string"
    },
    "apiUrl": {
      "type": "string"
    },
    "connector": {
      "identity": {
        "type": "boolean"
      },
      "network": {
        "type": "boolean"
      },
      "required": [
        "identity",
        "network"
      ],
      "type": "object"
    },
    "proxyUrl": {
      "type": "string"
    }
  },
  "required": [
    "apiUrl"
  ],
  "type": "object"
}